{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/theofidry/AliceBundle",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-pdo": "*",
        "api-platform/core": "~2.5.7",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6 || ^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "doctrine/orm": "^2.4.5",
        "dompdf/dompdf": "^0.8.5",
        "endroid/qr-code": "^3.9",
        "enqueue/sqs": "^0.10.9",
        "geoffreyrose/us-holidays": "^2.0",
        "league/csv": "^9.6",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/cors-bundle": "^1.5 || ^2.0",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0 || ^5.0",
        "ramsey/uuid": "^4.1",
        "scienta/doctrine-json-functions": "^4.3",
        "sonata-project/google-authenticator": "^2.3",
        "sroze/messenger-enqueue-transport": "^0.5.0",
        "symfony/amazon-mailer": "5.*",
        "symfony/asset": "4.4.*",
        "symfony/cache": "4.4.*",
        "symfony/config": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/filesystem": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/http-foundation": "4.4.*",
        "symfony/lock": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/mime": "4.4.*",
        "symfony/monolog-bridge": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/routing": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/security-csrf": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twilio/sdk": "^6.9",
        "vich/uploader-bundle": "^1.13"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "hautelook/alice-bundle": "^2.7",
        "phpstan/phpstan": "^0.12.19",
        "symfony/browser-kit": "4.4.*",
        "symfony/dom-crawler": "4.4.*",
        "symfony/maker-bundle": "^1.15",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/stopwatch": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/var-exporter": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/system/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "system-test": "./bin/phpunit tests/system",
        "unit-test": "phpunit tests/unit",
        "phpcs": "./vendor/bin/phpcs src"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}

